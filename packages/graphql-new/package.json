{
  "name": "@fuel-explorer/graphql-new",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "author": "Fuel Labs <contact@fuel.sh> (https://fuel.network/)",
  "license": "Apache-2.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "typings": "./src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "typings": "./dist/index.d.ts",
    "exports": {
      ".": {
        "require": "./dist/index.js",
        "default": "./dist/index.mjs"
      }
    },
    "files": ["dist"]
  },
  "scripts": {
    "build": "run-s codegen build:lib",
    "build:lib": "tsup",
    "codegen": "gql-gen -r dotenv/config --config codegen.ts",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "tsx ./src/cli.ts migrate",
    "inngest": "inngest-cli dev -u http://localhost:3002/api/inngest",
    "dev": "tsx watch src/server.ts",
    "sync:all": "tsx ./src/cli.ts sync --all",
    "sync:missing": "tsx ./src/cli.ts sync --missing"
  },
  "dependencies": {
    "@graphql-tools/schema": "^10.0.2",
    "@graphql-tools/stitch": "^9.0.3",
    "@graphql-tools/utils": "^10.0.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "cors": "^2.8.5",
    "dotenv": "16.4.4",
    "drizzle-orm": "^0.29.3",
    "graphql": "^16.8.1",
    "graphql-request": "6.1.0",
    "graphql-tag": "^2.12.6",
    "graphql-yoga": "5.1.1",
    "inngest": "3.14.0",
    "pg": "^8.11.3",
    "tai64": "^1.0.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-graphql-request": "^6.1.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@types/node": "^20.11.5",
    "@types/pg": "^8.11.0",
    "drizzle-kit": "^0.20.14",
    "inngest-cli": "0.25.2",
    "tsx": "4.7.0",
    "typescript": "^5.3.3"
  }
}
