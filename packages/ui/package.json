{
  "name": "@fuel-explorer/ui",
  "version": "0.0.1",
  "access": "public",
  "main": "./src/index.tsx",
  "types": "./src/index.tsx",
  "typings": "./src/index.tsx",
  "exports": {
    ".": "./src/index.tsx",
    "./tailwind-preset": "./src/theme/tailwind-preset.ts",
    "./styles.css": "./src/index.css",
    "./tokens.css": "./src/tokens.css",
    "./*": "./src/components/*/index.tsx"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./src/components/*/index.tsx"
      ],
      ".": [
        "./src/index.tsx"
      ],
      "tailwind-preset": [
        "./src/theme/tailwind-preset.ts"
      ],
      "types": [
        "./src/utils/types.ts"
      ]
    }
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "typings": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.js",
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "typings": "./dist/index.d.ts"
      },
      "./*": {
        "import": "./dist/components/*/index.js",
        "require": "./dist/components/*/index.js",
        "default": "./dist/components/*/index.js",
        "types": "./dist/components/*/index.d.ts",
        "typings": "./dist/components/*/index.d.ts"
      },
      "./tailwind-preset": {
        "import": "./dist/theme/index.js",
        "require": "./dist/theme/index.js",
        "default": "./dist/theme/index.js",
        "types": "./dist/theme/tailwind-preset.d.ts",
        "typings": "./dist/theme/tailwind-preset.d.ts"
      },
      "./types": {
        "types": "./dist/utils/types.d.ts",
        "typings": "./dist/utils/types.d.ts"
      },
      "./styles.css": "./dist/index.css",
      "./tokens.css": "./dist/tokens.css"
    },
    "typesVersions": {
      "*": {
        "*": [
          "./dist/components/*/index.d.ts"
        ],
        ".": [
          "./dist/index.d.ts"
        ],
        "tailwind-preset": [
          "./dist/theme/tailwind-preset.d.ts"
        ],
        "types": [
          "./dist/utils/types.d.ts"
        ]
      }
    },
    "files": [
      "dist"
    ]
  },
  "scripts": {
    "build:storybook": "pnpm ts:check && storybook build",
    "build": "tsup && tsc -p ./tsconfig.build.json --emitDeclarationOnly && pnpm add:clients",
    "build:watch": "tsup --watch",
    "add:clients": "node ./scripts/add-use-client.mjs",
    "dev:storybook": "storybook dev -p 6006",
    "lint": "eslint .",
    "test": "jest --verbose --passWithNoTests",
    "test:dev": "pnpm ts:check && pnpm test",
    "build-storybook": "storybook build",
    "ts:check": "tsc --noEmit",
    "generate:defs": "node scripts/create-defs.mjs"
  },
  "dependencies": {
    "@fontsource-variable/inter": "5.0.8",
    "@fuel-ts/math": "0.58.0",
    "@radix-ui/colors": "3.0.0-rc.4",
    "@radix-ui/react-accordion": "1.1.2",
    "@radix-ui/react-aspect-ratio": "1.0.3",
    "@radix-ui/react-dialog": "1.0.4",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-toast": "1.1.4",
    "@radix-ui/themes": "^1.1.2",
    "@react-aria/focus": "3.14.1",
    "@tabler/icons-react": "2.34.0",
    "@tailwindcss/typography": "0.5.10",
    "clsx": "2.0.0",
    "csstype": "3.1.2",
    "deepmerge-json": "1.5.0",
    "framer-motion": "10.16.4",
    "radix-ui-themes-with-tailwind": "1.2.6",
    "react": "^18.2.0",
    "react-aria": "3.28.0",
    "react-dom": "^18.2.0",
    "react-stately": "3.26.0",
    "react-use": "17.4.0",
    "tailwind-merge": "1.14.0",
    "tailwind-variants": "0.1.14",
    "tailwindcss-animate": "1.0.7",
    "tailwindcss-radix": "2.8.0",
    "tailwindcss-themer": "3.1.0"
  },
  "devDependencies": {
    "@chialab/esbuild-plugin-meta-url": "0.17.7",
    "@storybook/addon-a11y": "^7.4.3",
    "@storybook/addon-actions": "^7.4.3",
    "@storybook/addon-essentials": "^7.4.3",
    "@storybook/addon-interactions": "^7.4.3",
    "@storybook/addon-links": "^7.4.3",
    "@storybook/addon-storysource": "^7.4.3",
    "@storybook/addon-viewport": "7.4.3",
    "@storybook/addons": "^7.4.3",
    "@storybook/react": "^7.4.3",
    "@storybook/react-vite": "^7.4.3",
    "@storybook/testing-library": "^0.2.1",
    "@storybook/types": "^7.4.3",
    "@types/lodash": "4.14.198",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "10.4.16",
    "globby": "13.2.2",
    "lodash": "^4.17.21",
    "postcss": "8.4.30",
    "postcss-import": "15.1.0",
    "storybook": "^7.4.3",
    "storybook-addon-theme": "workspace:*",
    "tailwindcss": "3.3.3",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "5.2.2",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
