{
  "name": "@fuel-explorer/graphql",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "author": "Fuel Labs <contact@fuel.sh> (https://fuel.network/)",
  "license": "Apache-2.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "typings": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./sdk": "./src/graphql/generated/sdk.ts"
  },
  "typesVersions": {
    "*": {
      "sdk": ["./src/graphql/generated/sdk.ts"]
    }
  },
  "scripts": {
    "build:lib": "run-s codegen build:sdk",
    "build:sdk": "tsup",
    "ts:check": "tsc --noEmit",
    "codegen": "run-s codegen:*",
    "codegen:fuel": "gql-gen -r dotenv/config --config codegen.fuel.ts && ./scripts/fix-schema.sh",
    "codegen:gql-fuelcore": "gqlg --schemaFilePath ./src/graphql/schemas/fuelcore.graphql --destDirPath ./src/graphql/generated/fuelcore --ext graphql --includeCrossReferences --depthLimit 6",
    "codegen:sdk": "gql-gen -r dotenv/config --config codegen.sdk.ts",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "tsx ./src/cli.ts migrate",
    "db:push": "drizzle-kit push:pg",
    "db:setup": "run-s codegen db:generate",
    "db:clean": "tsx ./src/cli.ts clean-db",
    "dev:graphql": "APP=graphql pm2 start ecosystem-dev.config.cjs",
    "dev:syncer": "APP=syncer pm2 start ecosystem-dev.config.cjs",
    "pm2:start": "pm2 start ecosystem.config.cjs --update-env",
    "pm2:restart": "pm2 restart ecosystem.config.cjs --update-env",
    "pm2:reset": "pm2 delete ecosystem.config.cjs && pnpm server:start",
    "pm2:stop": "pm2 stop ecosystem.config.cjs",
    "pm2:delete": "pm2 delete ecosystem.config.cjs",
    "server:start": "pm2-runtime ecosystem.config.cjs",
    "sync:all": "tsx ./src/cli.ts sync --all",
    "sync:missing": "tsx ./src/cli.ts sync --missing",
    "sync:from-block": "tsx ./src/cli.ts sync --from",
    "sync:clean": "tsx ./src/cli.ts sync --clean",
    "sync:last": "tsx ./src/cli.ts sync --last",
    "prepare": "pnpm build:lib"
  },
  "peerDependencies": {
    "graphql-request": ">=6.1.0",
    "graphql": ">=16.8.2",
    "graphql-tag": ">=2.12.6"
  },
  "dependencies": {
    "@graphql-tools/load": "8.0.2",
    "@graphql-tools/load-files": "7.0.0",
    "@graphql-tools/merge": "9.0.4",
    "@graphql-tools/schema": "10.0.4",
    "@graphql-tools/stitch": "^9.2.9",
    "@graphql-tools/utils": "^10.2.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "amqplib": "0.10.4",
    "cors": "^2.8.5",
    "dayjs": "1.11.11",
    "dotenv": "16.4.5",
    "drizzle-orm": "^0.31.2",
    "ethers": "^6.13.0",
    "express": "5.0.0-beta.3",
    "fuels": "0.89.2",
    "graphql": "^16.8.2",
    "graphql-yoga": "5.3.1",
    "kafkajs": "2.2.4",
    "lodash": "^4.17.21",
    "node-fetch": "3.3.2",
    "npm-run-all": "^4.1.5",
    "pg": "^8.12.0",
    "pg-boss": "9.0.3",
    "pm2": "5.4.0",
    "shallow-equal-object": "1.1.1",
    "tai64": "^1.0.0",
    "winston": "3.13.0",
    "xstate": "^5.13.2",
    "yargs": "17.7.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@fuels/ts-config": "^0.20.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/introspection": "^4.0.3",
    "@graphql-codegen/schema-ast": "^4.0.2",
    "@graphql-codegen/typescript": "^4.0.7",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.2.1",
    "@types/amqplib": "0.10.5",
    "@types/lodash": "4.17.5",
    "@types/node": "^20.14.2",
    "@types/pg": "^8.11.6",
    "chalk": "5.3.0",
    "drizzle-kit": "^0.22.7",
    "gql-generator": "2.0.0",
    "graphql": "^16.8.2",
    "graphql-codegen-typescript-mock-data": "3.8.0",
    "graphql-config": "5.0.3",
    "graphql-request": "6.1.0",
    "graphql-tag": "^2.12.6",
    "tsup": "8.1.0",
    "tsx": "4.15.4",
    "typescript": "^5.4.5"
  }
}
