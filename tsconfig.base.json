{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@fuels/ts-config/base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "importsNotUsedAsValues": "error"
  },
  "exclude": [
    "node_modules",
    "**/**/node_modules",
    "dist",
    "**/**/dist",
    "coverage",
    "storybook-static",
    "generated",
    "**/**/generated",
    "**/.*"
  ]
}
